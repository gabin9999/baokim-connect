{
    "name": "duynv/baokim-connect",
    "description": "Generate Connection with BAOKIMApi",
    "type": "composer-plugin",
    "minimum-stability": "dev",
    "require": {
        "composer-plugin-api": "^2.0",
        "php": ">=5.8"
    },
    "authors": [
        {
            "name": "Mulligan",
            "email": "duynv1499@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "class": "Connect",
        "laravel": {
            "providers": [
                "ConnectionServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "composer/composer": "^2.0"
    }
}
