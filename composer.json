{
    "name": "duynv/baokim-connect",
    "description": "Generate Connection with BAOKIMApi",
    "type": "composer-plugin",
    "minimum-stability": "dev",
    "require": {
        "composer/installers": "^2.4.2",
        "php": ">=5.8"
    },
    "authors": [
        {
            "name": "Mulligan",
            "email": "duynv1499@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "class": "src\\ConnectionServiceProvider",
        "laravel": {
            "providers": [
                "src\\ConnectionServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    }
}
